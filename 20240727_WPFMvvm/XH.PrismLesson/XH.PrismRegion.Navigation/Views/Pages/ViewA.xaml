<UserControl x:Class="XH.PrismRegion.Navigation.Views.Pages.ViewA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:XH.PrismRegion.Navigation.Views.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Text="ViewA" FontSize="40" Foreground="Orange" />

        <Button Content="Close" Command="{Binding CloseTabCommand}" />
    </StackPanel>
</UserControl>
