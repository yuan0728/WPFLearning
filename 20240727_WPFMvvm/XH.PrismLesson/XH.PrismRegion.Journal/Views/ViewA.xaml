<UserControl x:Class="XH.PrismRegion.Journal.Views.ViewA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:XH.PrismRegion.Journal.Views"
             mc:Ignorable="d" FontSize="40"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Text="View A" Foreground="Orange" />
        <Button Content="向前" Command="{Binding ForwordCommand}"/>
        <Button Content="向后" Command="{Binding GoBackCommand}"/>
    </StackPanel>
</UserControl>
